<p id="notice"><%= notice %></p>

<h1>Ascetic Practices</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ascetic_practices.each do |ascetic_practice| %>
      <tr>
        <td><%= ascetic_practice.description %></td>
        <td><%= link_to 'Show', ascetic_practice %></td>
        <td><%= link_to 'Edit', edit_ascetic_practice_path(ascetic_practice) %></td>
        <td><%= link_to 'Destroy', ascetic_practice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ascetic Practice', new_ascetic_practice_path %>
